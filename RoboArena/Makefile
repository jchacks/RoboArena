  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -O2 -fPIC -F /usr/local/Cellar/python@3.8/3.8.12_1/Frameworks
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -O2 -fPIC -std=c++17 -F /usr/local/Cellar/python@3.8/3.8.12_1/Frameworks
